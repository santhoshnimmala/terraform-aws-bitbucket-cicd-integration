variable bucketname {
  type        = string
  default     = "name-of-s3-bucket"
  description = "varible for s3 bucket"
}
variable project_url {
  type        = string
  default     = "https://xxxxxx.yyyy.com/stash"
  description = "varible for bitbucket base url"
}

variable token {
  type        = string
  default     = "XXXXXX-YYYY-ZZZZZZ"
  description = "varible for bitbucket token"
}


